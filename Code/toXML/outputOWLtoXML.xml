<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:ex="http://example.com/ontology#">
  <ex:Device rdf:about="http://example.com/ontology#Device1">
    <ex:hasChannel>
      <ex:Channel rdf:about="http://example.com/ontology#Channel2">
        <ex:samplingInterval>5s</ex:samplingInterval>
        <ex:channelAddress>/chan/two</ex:channelAddress>
        <ex:description>Channel Two</ex:description>
        <ex:loggingInterval>10s</ex:loggingInterval>
        <ex:unit>kW</ex:unit>
        <ex:channelId>c2</ex:channelId>
      </ex:Channel>
    </ex:hasChannel>
    <ex:hasChannel>
      <ex:Channel rdf:about="http://example.com/ontology#Channel1">
        <ex:samplingInterval>5s</ex:samplingInterval>
        <ex:channelAddress>/chan/one</ex:channelAddress>
        <ex:description>Channel One</ex:description>
        <ex:loggingInterval>5s</ex:loggingInterval>
        <ex:unit>kW</ex:unit>
        <ex:channelId>c1</ex:channelId>
      </ex:Channel>
    </ex:hasChannel>
    <ex:deviceAddress>/dev/one</ex:deviceAddress>
    <ex:description>Device One</ex:description>
    <ex:deviceId>1</ex:deviceId>
  </ex:Device>
  <ex:Device rdf:about="http://example.com/ontology#Device2">
    <ex:hasChannel>
      <ex:Channel rdf:about="http://example.com/ontology#Channel3">
        <ex:samplingInterval>15s</ex:samplingInterval>
        <ex:channelAddress>/chan/three</ex:channelAddress>
        <ex:description>Channel Three</ex:description>
        <ex:loggingInterval>15s</ex:loggingInterval>
        <ex:unit>MW</ex:unit>
        <ex:channelId>c3</ex:channelId>
      </ex:Channel>
    </ex:hasChannel>
    <ex:deviceAddress>/dev/two</ex:deviceAddress>
    <ex:description>Device Two</ex:description>
    <ex:deviceId>2</ex:deviceId>
  </ex:Device>
</rdf:RDF>
